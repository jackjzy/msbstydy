<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.shop.FfwyShopPhotoMapper">
    
    <resultMap type="FfwyShopPhoto" id="FfwyShopPhotoResult">
        <result property="photoId"    column="photo_id"    />
        <result property="shopId"    column="shop_id"    />
        <result property="phototPath"    column="photot_path"    />
        <result property="title" column="title"></result>
        <result property="time" column="timeing"></result>
        <result property="createTime" column="create_time"></result>
        <result property="status" column="status"></result>
    </resultMap>

    <sql id="selectFfwyShopPhotoVo">
        select photo_id, shop_id, photot_path from ffwy_shop_photo
    </sql>

    <sql id="SELECT_SHOP_PHOTO">
        select photo_id, shop_id, photot_path,title,timeing,status,create_time
            from ffwy_shop_photo
    </sql>

    <select id="selectFfwyShopPhotoList" parameterType="FfwyShopPhoto" resultMap="FfwyShopPhotoResult">
        <include refid="SELECT_SHOP_PHOTO"/>
        <where>  
            <if test="photoId != null "> and photo_id = #{photoId}</if>
            <if test="shopId != null "> and shop_id = #{shopId}</if>
            <if test="phototPath != null  and phototPath != ''"> and photot_path = #{phototPath}</if>
            <if test="title != null and title != ''">title=#{title},</if>
            <if test="time != null ">timeing=#{time},</if>
            <if test="beginTime != null "> and create_time &gt;=#{beginTime} </if>
            <if test="endTime != null "> and create_time &lt;= #{endTime} </if>
            <if test="createTime != null ">create_time=#{createTime},</if>
            <if test="status != null and status != ''">status=#{status},</if>

        </where>
        ORDER BY timeing
    </select>
    
    <select id="selectFfwyShopPhotoById" parameterType="Long" resultMap="FfwyShopPhotoResult">
        <include refid="SELECT_SHOP_PHOTO"/>
        where photo_id = #{photoId}
    </select>

    <select id="selectFfwyShopPhotoListByShopId" parameterType="Long" resultMap="FfwyShopPhotoResult">
        <include refid="SELECT_SHOP_PHOTO"/>
        where shop_id = #{shopId}
    </select>
        
    <insert id="insertFfwyShopPhoto" parameterType="FfwyShopPhoto" useGeneratedKeys="true" keyProperty="photoId">
        insert into ffwy_shop_photo
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="shopId != null">shop_id,</if>
            <if test="phototPath != null and phototPath != ''">photot_path,</if>
            <if test="title != null and title != ''">title,</if>
            <if test="time != null ">timeing,</if>
            <if test="createTime != null ">create_time,</if>
            <if test="status != null and status != ''">status,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="shopId != null">#{shopId},</if>
            <if test="phototPath != null and phototPath != ''">#{phototPath},</if>
            <if test="title != null and title != ''">#{title},</if>
            <if test="time != null ">#{time},</if>
            <if test="createTime != null ">#{createTime},</if>
            <if test="status != null and status != ''">#{status},</if>
         </trim>
    </insert>
    <insert id="insertFfwyShopPhotos" parameterType="java.util.List" >
        insert into ffwy_shop_photo(shop_id,photot_path,title,timeing,create_time,status)
        VALUES
        <foreach collection="list" index="index" item="FfwyShopPhoto" separator=",">
            (
            #{FfwyShopPhoto.shopId}, #{FfwyShopPhoto.phototPath},#{FfwyShopPhoto.title}, #{FfwyShopPhoto.time},
             #{FfwyShopPhoto.createTime}, #{FfwyShopPhoto.status}
            )

        </foreach>

    </insert>


    <update id="updateFfwyShopPhoto" parameterType="FfwyShopPhoto">
        update ffwy_shop_photo
        <trim prefix="SET" suffixOverrides=",">
            <if test="shopId != null">shop_id = #{shopId},</if>
            <if test="phototPath != null and phototPath != ''">photot_path = #{phototPath},</if>
            <if test="title != null and title != ''">title=#{title},</if>
            <if test="time != null ">timeing=#{time},</if>
            <if test="createTime != null ">create_time=#{createTime},</if>
            <if test="status != null and status != ''">status=#{status},</if>
        </trim>
        where photo_id = #{photoId}
    </update>

    <delete id="deleteFfwyShopPhotoById" parameterType="Long">
        delete from ffwy_shop_photo where photo_id = #{photoId}
    </delete>

    <delete id="deleteFfwyShopPhotoByIds" parameterType="String">
        delete from ffwy_shop_photo where photo_id in 
        <foreach item="photoId" collection="array" open="(" separator="," close=")">
            #{photoId}
        </foreach>
    </delete>
</mapper>