<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.aftersale.FfwyAfterPhotoMapper">
    
    <resultMap type="FfwyAfterPhoto" id="FfwyAfterPhotoResult">
        <result property="afterPhotoId"    column="after_photo_id"    />
        <result property="afterSaleId"    column="after_sale_id"    />
        <result property="afterPhoto"    column="after_photo"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateTime"    column="update_time"    />
    </resultMap>
    
    <insert id="insertFfwyAfterPhotos" parameterType="java.util.List">
        insert into
        ffwy_after_photo(after_photo_id,after_sale_id,after_photo,create_time,update_time)
        values
        <foreach collection="list" index="index" item="FfwyAfterPhoto" separator=",">
            (#{FfwyAfterPhoto.afterPhotoId},#{FfwyAfterPhoto.afterSaleId},#{FfwyAfterPhoto.afterPhoto},#{createTime},#{updateTime})
        </foreach>
    </insert>

    <insert id="insertFfwyAfterPhoto" parameterType="FfwyAfterPhoto" useGeneratedKeys="true" keyProperty="afterPhotoId">
        insert into ffwy_after_photo
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="afterSaleId != null">after_sale_id,</if>
            <if test="afterPhoto != null">after_photo,</if>
            <if test="createTime != null">create_time,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="afterSaleId != null">#{afterSaleId},</if>
            <if test="afterPhoto != null">#{afterPhoto},</if>
            <if test="createTime != null">#{createTime},</if>
        </trim>
    </insert>

</mapper>